<package>
  <name>rosprolog</name>
  <version>1.0.0</version>
  <description>
    Script to execute Prolog and set the Prolog library paths
    to other ROS Prolog packages.
  </description>
  <maintainer email="knowrob@tenorth.de">Moritz Tenorth</maintainer>

  <license>BSD, GPL</license>

  <url type="website">http://www.ros.org/wiki/rosprolog</url>
  <url type="bugtracker">https://github.com/knowrob/knowrob/issues</url>

  <author>Lorenz Moesenlechner</author>

  <buildtool_depend>catkin</buildtool_depend>

  <run_depend>swi-prolog</run_depend>
  <run_depend>swi-prolog-java</run_depend>
  
  <export>
    <rosdoc config="rosdoc.yaml"/>
  </export>

</package>